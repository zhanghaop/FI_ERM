		<property name="operateCode" value="edit" />
		<property name="resourceCode" value="ermloanservice" />
		<property name="interceptor" ref="interceptor"/>

	</bean>
	<bean id="saveaction" class="nc.ui.erm.billpub.action.BillSaveAction">
