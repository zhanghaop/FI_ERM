    <selectSql>select 
&apos;借款单 名称 &apos; + T2.billtypename + &apos; 单据编号 &apos; + T0.djbh + &apos; 借款部门 &apos; + T7.deptname + &apos; 借款人 &apos; + T4.psnname as &quot;TITLE&quot;,
&apos;单据编号 &apos; + T0.djbh + &apos; 交易类型 &apos; + T2.billtypename + &apos; 摘要 &apos; + T0.zy + &apos; 借款部门 &apos; + T7.deptname + &apos; 借款人 &apos; + T4.psnname  + &apos; 资金计划项目 &apos; + T0.cashproj + &apos; 收支项目 &apos; + T5.costname + &apos; 借款本币金额 &apos; + cast((T0.bbje) as char(20)) + &apos; 单据日期 &apos; + T0.djrq + &apos; 审核日期 &apos; + T0.shrq + &apos; 结算日期 &apos; + T0.jsrq + &apos; 结算方式 &apos; + T6.balanname + &apos; 票据号 &apos; + T0.pjh + &apos; 最迟还款日 &apos; + T0.zhrq + &apos; 清账状态 &apos; + cast((T0.qzzt) as char(20)) as &quot;CONTENT&quot;,
T0.pk_corp as &quot;CORPID&quot;,
T1.unitname as &quot;CORP&quot;,
T7.deptname as &quot;DEPT&quot;,
T2.billtypename as &quot;BUSITYPE&quot;,
T0.djbh as &quot;BILLCODE&quot;,
T3.user_name as &quot;BILLMAKER&quot;,
T0.djrq as &quot;MAKEDATE&quot;</selectSql>
    <fromSql> from 
er_jkzb T0 left outer join bd_corp T1 on T0.pk_corp = T1.pk_corp 
  left outer join bd_billtype T2 on T0.djlxbm = T2.pk_billtypecode 
  left outer join sm_user T3 on T0.lrr = T3.cuserid 
  left outer join bd_psndoc T4 on T0.jkbxr = T4.pk_psndoc 
  left outer join bd_costsubj T5 on T0.szxmid = T5.pk_costsubj 
  left outer join bd_balatype T6 on T0.jsfs = T6.pk_balatype 
  left outer join bd_deptdoc T7 on T0.deptid = T7.pk_deptdoc 
  where T0.dr=0 </fromSql>
